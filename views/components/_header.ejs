<!-- Navbar -->
<nav class="bg-white rounded-lg shadow mt-4 mx-4">
  <div
    class="max-w-screen-xl flex flex-wrap items-center justify-between mx-auto p-4"
  >
    <a href="#" class="flex items-center space-x-3 rtl:space-x-reverse">
      <img src="/img/logo.png" class="h-20" alt="PPC logo" />
    </a>
    <div
      class="flex items-center md:order-2 space-x-3 md:space-x-0 rtl:space-x-reverse"
    >
      <button
        type="button"
        class="flex text-sm bg-white-800 rounded-full md:me-0 <%= typeof user ==='undefined' ? 'hidden': 'block' %>"
        id="user-menu-button"
        aria-expanded="false"
        data-dropdown-toggle="user-dropdown"
        data-dropdown-placement="bottom"
      >
        <span class="sr-only">Open user menu</span>
        <ion-icon name="person-circle-outline" class="w-8 h-8"></ion-icon>
      </button>
      <a
      type="button"
      class="flex text-sm bg-white-800 rounded-full md:me-0 relative"
      id="user-menu-button"
      href="/checkout"
    >
      <div id="notify-cart" class="absolute z-50" style="right:-4px;top:-2px;">
        <p
        id="cart-counter"
          class="flex h-4 w-4 items-center justify-center rounded-full bg-blue-500 text-xs text-white font-bold"
        >
          
        </p>
      </div>
      <span class="sr-only">Open cart</span>
      <ion-icon name="cart-outline" class="w-8 h-8"></ion-icon>
    </a>
      <!-- Dropdown menu -->
      <div
        class="z-50 hidden my-4 text-base list-none bg-white divide-y divide-gray-100 rounded-lg shadow"
        id="user-dropdown"
      >
        <div class="px-4 py-3">
          <span class="block text-sm text-gray-900"
            ><%=typeof user !=='undefined' && user ? user.name : '' %>
          </span>
          <span class="block text-sm text-gray-500 truncate"
            ><%=typeof user !=='undefined' && user ? user.email : '' %></span
          >
        </div>
        <ul class="py-2" aria-labelledby="user-menu-button">
          <li>
            <a
              href="/profile"
              class="px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 flex items-center space-x-2"
              ><ion-icon
                name="person-circle-outline"
                class="w-6 h-6 text-gray-500"
              ></ion-icon>
              <span class="text-center">Profile</span></a
            >
          </li>
          <% if(typeof user !=='undefined' && user?.role === "user") {%>
          <li>
            <a
              href="/me/orders"
              class="px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 flex items-center space-x-2"
              ><ion-icon
                name="receipt-outline"
                class="w-6 h-6 text-gray-500"
              ></ion-icon>
              <span class="text-center">Orders</span></a
            >
          </li>
          <% } %>
          <% if(typeof user !=='undefined' && user?.role === "admin") {%>
          <li>
            <a
              href="/admin/dashboard/products"
              class="px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 flex items-center space-x-2"
              >
              <ion-icon name="cube-outline" class="w-6 h-6 text-gray-500"></ion-icon>
              <span class="text-center">Dashboard</span></a
            >
          </li>
          <% } %>
          <li>
            <a
              id="logout-btn"
              style="cursor: pointer"
              class="px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 flex items-center space-x-2"
              ><ion-icon
                name="log-out-outline"
                class="w-6 h-6 text-red-500"
              ></ion-icon>
              <span class="text-center text-red-500">Log out</span></a
            >
          </li>
        </ul>
      </div>
      <button
        data-collapse-toggle="navbar-user"
        type="button"
        class="inline-flex items-center p-2 w-10 h-10 justify-center text-sm text-gray-500 rounded-lg md:hidden hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-gray-200"
        aria-controls="navbar-user"
        aria-expanded="false"
      >
        <span class="sr-only">Open main menu</span>
        <svg
          class="w-5 h-5"
          aria-hidden="true"
          xmlns="http://www.w3.org/2000/svg"
          fill="none"
          viewBox="0 0 17 14"
        >
          <path
            stroke="currentColor"
            stroke-linecap="round"
            stroke-linejoin="round"
            stroke-width="2"
            d="M1 1h15M1 7h15M1 13h15"
          />
        </svg>
      </button>

      <!-- Login Button -->
      <!-- Modal toggle -->
      <button
        id="login-btn"
        data-modal-target="authentication-modal"
        data-modal-toggle="authentication-modal"
        class="block border border-mulledWine font-medium rounded-lg text-sm px-5 py-2.5 text-center primary-btn btn-animated <%= typeof user !=='undefined' && user ? 'hidden': 'block' %>"
        type="button"
      >
        Login
      </button>

      <!-- Login Modal -->
      <div
        id="authentication-modal"
        tabindex="-1"
        aria-hidden="true"
        class="hidden overflow-y-auto overflow-x-hidden fixed top-0 right-0 left-0 z-50 justify-center items-center w-full md:inset-0 h-[calc(100%-1rem)] max-h-full"
        style="margin-left: 0px"
      >
        <div class="relative p-4 w-full max-w-md max-h-full">
          <!-- Modal content -->
          <div class="relative bg-white rounded-lg shadow">
            <!-- Modal header -->
            <div
              class="flex items-center justify-between p-4 md:p-5 border-b rounded-t"
            >
              <h3 class="text-xl font-semibold text-gray-900">
                Sign in to our platform
              </h3>
              <button
                id="login-modal-close-btn"
                type="button"
                class="end-2.5 text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm w-8 h-8 ms-auto inline-flex justify-center items-center"
                data-modal-hide="authentication-modal"
              >
                <svg
                  class="w-3 h-3"
                  aria-hidden="true"
                  xmlns="http://www.w3.org/2000/svg"
                  fill="none"
                  viewBox="0 0 14 14"
                >
                  <path
                    stroke="currentColor"
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="2"
                    d="m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6"
                  />
                </svg>
                <span class="sr-only">Close modal</span>
              </button>
            </div>
            <!-- Modal body -->
            <div class="p-4 md:p-5">
              <form class="space-y-4 form" id="login-form">
                <div>
                  <label
                    for="email"
                    class="block mb-2 text-sm font-medium text-gray-900"
                    >Your email</label
                  >
                  <input
                    type="email"
                    name="email"
                    id="email-login"
                    class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5"
                    placeholder="name@company.com"
                    required
                  />
                </div>
                <div>
                  <label
                    for="password"
                    class="block mb-2 text-sm font-medium text-gray-900"
                    >Your password</label
                  >
                  <input
                    type="password"
                    name="password"
                    id="password-login"
                    placeholder="••••••••"
                    class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5"
                    required
                  />
                </div>
                <div class="flex justify-between">
                  <div class="flex items-start">
                    <div class="flex items-center h-5">
                      <input
                        id="show-password-login"
                        type="checkbox"
                        value=""
                        class="w-4 h-4 border border-gray-300 rounded bg-gray-50 focus:ring-3 focus:ring-blue-300"
                      />
                    </div>
                    <label
                      for="remember"
                      class="ms-2 text-sm font-medium text-gray-900"
                      >Show password</label
                    >
                  </div>
                  <a href="/forgotpassword" class="text-sm text-blue-700 hover:underline"
                    >Forgot password?</a
                  >
                </div>
                <button
                  id="login-submit"
                  type="submit"
                  class="w-full text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
                >
                  Login
                </button>
                <div class="text-sm font-medium text-gray-500">
                  Not registered?
                  <a
                    href="#"
                    data-modal-target="signup-modal"
                    data-modal-toggle="signup-modal"
                    data-modal-hide="authentication-modal"
                    class="text-blue-700 hover:underline"
                    id="create-account-btn"
                    >Create account</a
                  >
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>

      <!-- Sign Up Modal -->
      <div
        id="signup-modal"
        tabindex="-1"
        aria-hidden="true"
        class="hidden overflow-y-auto overflow-x-hidden fixed top-0 right-0 left-0 z-50 justify-center items-center w-full md:inset-0 h-[calc(100%-1rem)] max-h-full"
